{% extends 'base.html.twig' %}

{% block title %}Emailsubscription{% endblock %}

{% block body %}
    <h1>Emailsubscription</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emailsubscription.id }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{{ emailsubscription.actif ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ emailsubscription.email }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ emailsubscription.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ emailsubscription.prenom }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ emailsubscription.type }}</td>
            </tr>
            <tr>
                <th>Date_add</th>
                <td>{{ emailsubscription.dateAdd ? emailsubscription.dateAdd|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('emailsubscription_index') }}">back to list</a>

    <a href="{{ path('emailsubscription_edit', {'id': emailsubscription.id}) }}">edit</a>

    {{ include('emailsubscription/_delete_form.html.twig') }}
{% endblock %}
